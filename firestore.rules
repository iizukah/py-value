rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /workbooks/{workbookId} {
      allow read: if true;
      allow write: if false;
    }
    match /workbooks/{workbookId}/questions/{questionId} {
      allow read: if true;
      allow write: if false;
    }
  }
}
